@model  TP_Final_BD_MVC_Session5.Models.Esports

@{
    ViewBag.Title = "Contact";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

@do
{
    <br />
    @Model.Nom.ToString();
    <br />
    @Model.DateCreation.ToLongDateString();
    <br />
    
    <img src="~/Images/@Model.Logo.ToString()" />
    <br />

} while (Model.Next());


